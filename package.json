  },
  "homepage": "https://github.com/getHashSet/boilerplate#readme",
  "dependencies": {
    "concurrently": "^5.2.0",
    "aws-sdk": "^2.663.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.9.10",
    "morgan": "^1.8.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.2",
