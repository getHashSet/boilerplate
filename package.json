  "bugs": {
    "url": "https://github.com/getHashSet/boilerplate/issues"
  },
  "homepage": "https://github.com/getHashSet/boilerplate#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.6",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "if-env": "^1.0.4",
    "react-scripts": "^3.3.0"
