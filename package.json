{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "*by Matthew Carpenter*",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:prod",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon server.js --ignore 'boilerplate/*'\" \"npm run client\"",
    "client": "cd boilerplate && npm run start",
    "install": "cd boilerplate && npm install",
    "build": "cd boilerplate && npm run build",
    "prod": "node server.js",
    "heroku-postbuild": "npm run build",
    "seed": "node utils/scripts/seeds.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getHashSet/boilerplate.git"
  },
  "author": "",
