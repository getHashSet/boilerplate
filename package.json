{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "*by Matthew Carpenter*",
  "main": "server.js",
  "scripts": {
    "start": "npm run start:prod",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon server.js --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "seed": "node server_utilities/seed_files/seeds.js",
    "prod": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getHashSet/boilerplate.git"
  },
